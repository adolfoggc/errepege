<%= render '/public/menu' %>


<div style="margin: 0px;">
	<% y = 0 %>
	<% while y<5 %>
		<% x = 0 %>
		<% while x<5 %>
			<% if @room[y][x] == 0 %>
				<div style="width: 100px; height: 100px; background-color: blue; display: inline-block; border-color: black; border-width: 10px; border-style: solid;">
				</div>
			<% elsif @room[y][x] == 1 %>
				<div style="width: 100px; height: 100px; background-color: red; display: inline-block; border-color: black; border-width: 10px; border-style: solid;">
				</div>
			<% end %>
			<% x+=1%>
		<% end %>
		<br>
		<% y+=1%>
	<% end %>
</div>